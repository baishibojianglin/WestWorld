<view class="content"><w-loading class="vue-ref" vue-id="1" text mask="true" click="true" data-ref="loading" bind:__l="__l"></w-loading><view class="uni-margin-wrap"><uni-swiper-dot vue-id="2" info="{{adList}}" current="{{current}}" mode="{{mode}}" field="ad_name" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="swiper-box" indicator-dots="{{false}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{adList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view data-event-opts="{{[['tap',[['toAdDetail',['$0'],[[['adList','',index,'ad_link']]]]]]]}}" class="swiper-item" style="{{'background:'+(item.bgcolor+' url('+item.ad_pic+')')+';'}}" bindtap="__e"><image style="width:100%;" src="{{item.ad_pic}}" mode="aspectFill"></image></view></swiper-item></block></swiper></uni-swiper-dot></view><page-head vue-id="3" title="{{title}}" bind:__l="__l"></page-head><view class="uni-list"><block wx:for="{{listData}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['toVenueDetail',['$0'],[[['listData','',key]]]]]]]}}" bindtap="__e"><view class="uni-media-list"><image class="uni-media-list-logo" src="{{value.thumb}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{''+value.venue_name+''}}<text style="float:right;margin-right:20rpx;">{{value.distance}}</text></view><view class="uni-media-list-text-bottom"><text>{{value.address}}</text><text>{{value.venue_phone}}</text></view></view></view></view></block></view><uni-load-more vue-id="4" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more></view>